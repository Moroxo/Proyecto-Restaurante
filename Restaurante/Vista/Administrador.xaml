<Window x:Class="Restaurante.Vista.Administrador"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Restaurante.Vista"
        mc:Ignorable="d"
        Title="Administrador" Height="497" Width="909">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="235*"/>
            <ColumnDefinition Width="29*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top" Grid.ColumnSpan="2">
            <Button Content="Administrar usuarios" Height="30" Name="AdmUsuario" Click="AdmUsuario_Click"/>
            <Button Content="Administrar mesas" Height="30" Name="AdmMesas" Click="AdmMesas_Click"/>
        </StackPanel>
        <Frame x:Name="VistaAdm" Margin="0,35,0,0" Grid.ColumnSpan="2"></Frame>
    </Grid>
</Window>
