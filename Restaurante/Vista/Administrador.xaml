<Window x:Class="Restaurante.Vista.Administrador"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Restaurante.Vista"
        mc:Ignorable="d"
        Title="Administrador" Height="497" Width="909">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="235*"/>
            <ColumnDefinition Width="29*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top" Grid.ColumnSpan="2">
            <Button Content="Usuarios" Height="30" Name="AdmUsuario" Click="AdmUsuario_Click" Margin="0,2,0,3" Width="55"/>
            <Button Content="Mesas" Height="30" Name="AdmMesas" Click="AdmMesas_Click" Margin="0,2,0,3" Width="71"/>
            <Button Content="Insumos" Height="30" x:Name="AdmInsumos" Click="AdmInsumos_Click" Margin="0,2,0,3" Width="79"/>
            <Button Content="Recetas" Height="30" x:Name="AdmRecetas" Click="AdmRecetas_Click" Margin="0,2,0,3" Width="71"/>
        </StackPanel>
        <Frame x:Name="VistaAdm" Margin="0,35,0,0" Grid.ColumnSpan="2"></Frame>
    </Grid>
</Window>
